<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:q="urn:import:com.qwertify.client.ui.widget"
	xmlns:e="urn:import:com.google.gwt.editor.ui.client">
	
	<ui:with field='res' type='com.qwertify.client.ui.resources.Resources'/>

	<ui:style>
	</ui:style>
	
	<g:HTMLPanel>
		Name:
		<e:ValueBoxEditorDecorator ui:field="name">
			<e:valuebox>
				<q:TextBox />
			</e:valuebox>
		</e:ValueBoxEditorDecorator>
		Job Title:
		<e:ValueBoxEditorDecorator ui:field="jobTitle">
			<e:valuebox>
				<q:TextBox />
			</e:valuebox>
		</e:ValueBoxEditorDecorator>
		Age:
		<e:ValueBoxEditorDecorator ui:field="age">
			<e:valuebox>
				<q:IntegerBox />
			</e:valuebox>
		</e:ValueBoxEditorDecorator>
		
	</g:HTMLPanel>
</ui:UiBinder> 